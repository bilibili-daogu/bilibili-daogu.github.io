#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/blog/sci-fi-blog/node_modules/.store/browserify@11.2.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/blog/sci-fi-blog/node_modules/.store/browserify@11.2.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserify@11.2.0/node_modules/browserify/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../browserify@11.2.0/node_modules/browserify/bin/cmd.js" "$@"
fi
